{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noUnusedLocals": true,
    "lib": ["es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "exclude": ["fixtures"]
}